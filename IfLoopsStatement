If Loops Statements

using Unity Engine;
using System.Collections;

public class WhileLoops : MonoBehaviour {
	int i = 0; 		// public class is called a class, program and is a construct inside which things happen.
	public string[] colors;//public string is the return.
			       

	// Use this for initialization
	void Start () {
		//Set i equal to example 0 or numbers to run a fuction
		//While loops are like other statments.  
		// The while (i<10) or while (i< colors.Length) can be used for. 
		//The length is a parameter and an array. 
		//while loops executes the content of the loops and checks the condition before executing the content.  
		while (i< colors.Length) 
		{
			
			print(colors[i]);
			i++;// i++ is the return of the original value and then increments by one. 
		}

		print(colors[0]);// To the print the colors.
		print(colors[1]);//The example Of (i<10), the 10 as the elements to be setted numbers are meaning of the colors.
		print(colors[2]);
		print(colors[3]);
		print(colors[4]);
		print(colors[5]);
		print(colors[6]);
		print(colors[7]);
		print(colors[8]);
		print(colors[9]);
		print(colors[10];
		
		print("Done")
		
		// You can put an any kind of name in between the (i< and .Length)
		i = 0;
		while (i<animals.Length)
		{

		print(animals[i]);
		i ++;
		}
		
		i =0;
