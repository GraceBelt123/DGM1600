using UnityEngine;
using System.Collections;

public class ForeachLoops : MonoBehavior {

    public string[] objects;
    public string[] books;
    public string[] cats;
    public string[] shoes;
    public string[] notebooks;
    public string[] dogs;
    public string[] heels;
    public string[] wormbooks;
    
    //item is an identifier and a variable name and a keywork, meaning the variable type.
    //The collection is an identifier that doesn't even exist. So it has been replaced with an item that does exist. 
    //The datatype needs to be the same datatype, such as gameobject. 
    //The number of the items in the list that are zero from what he talked about in the class. 
    //When you add again object that is called referencing. 
    //At least it's not a start function. Its a void check data. 
    //Infinite loop is a list of instruction, no, it's more like sequence and an order. 
    //Busy wait can be called Infinite loop. 

    //Use this for initialization
    void Start() {
        //objectsInObjects is the item. 
        //If the item is equal to an item in the list, then the loop will check it for print what you want to print. 
       
       foreach (string objectsInObjects in objects) {
                if (objects == objectsInObjects)
                {
                     print("I have a match, it is" + objectsInObjects);
                }
                else
                {
                print(objectsInObjects + "is not a match");
                }

        foreach (string item in favorites) {
                if (item == "books")
